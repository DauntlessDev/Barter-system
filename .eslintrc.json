{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len":  ["error", { "code": 150 }],
        "no-unused-vars": 0,
        "no-use-before-define": 0
    }
}
